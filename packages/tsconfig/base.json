{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Shared Base",
    "compilerOptions": {
        "target": "ES2021",
        "module": "ESNext",
        "lib": [
            "dom",
            "dom.iterable",
            "ES2021"
        ],
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    }
}